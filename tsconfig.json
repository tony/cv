{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["dom", "dom.iterable", "es6", "es2019", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "es2020",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "experimentalDecorators": true,
    "sourceMap": true,
    "paths": {
      "@tony/cv-ui": ["./packages/ui"],
      "@tony/cv-ui-range-slider": ["./packages/ui-range-slider"],
      "@tony/cv-react": ["./packages/react"],
      "@tony/cv-lib": ["./packages/lib"],
      "@tony/cv-data": ["./packages/data"],
      "@tony/cv-nav": ["./packages/nav"],
      "@tony/cv-scripts": ["./packages/scripts"],
      "@tony/cv-chart-react-billboard": ["./packages/chart-react-billboard"],
      "@tony/cv-chart-react-carbon": ["./packages/chart-react-carbon"],
      "@tony/cv-chart-react-nivo": ["./packages/chart-react-nivo"],
      "@tony/cv-chart-react-plotly": ["./packages/chart-react-plotly"],
      "@tony/cv-chart-react-victory": ["./packages/chart-react-victory"],
      "@tony/cv-chart-react-vis": ["./packages/chart-react-vis"]
    }
  },
  "references": [
    { "path": "packages/ui" },
    { "path": "packages/ui-range-slider" },
    { "path": "packages/react" },
    { "path": "packages/lib" },
    { "path": "packages/data" },
    { "path": "packages/nav" },
    { "path": "packages/scripts" },
    { "path": "packages/chart-react-billboard" },
    { "path": "packages/chart-react-carbon" },
    { "path": "packages/chart-react-nivo" },
    { "path": "packages/chart-react-plotly" },
    { "path": "packages/chart-react-victory" },
    { "path": "packages/chart-react-vis" }
  ]
}
